{{>header}}
   <div class="container mt-4">
    <div class="row">
      {{#each blogs}}
        <div class="col sm-3 mt-2">
           <div class="card adjust-img">
  <img src="{{this.image}}" class="card-img-top" alt="{{this.title}}">
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
 
    <a href="/detail/{{this._id}}" class="btn btn-primary">Read more</a>
  </div>
</div>
        </div>
        {{/each}}  
        </div>

    </div>
{{>footer}}